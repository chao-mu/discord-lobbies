{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "dojo-pairer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --check . && eslint --ext .ts src",
    "deploy": "node --require dotenv/config dist/util/deploy.js",
    "lint:fix": "prettier --write . && eslint --ext .ts --fix src",
    "start": "node --require dotenv/config dist/index.js"
  },
  "dependencies": {
    "@discordjs/core": "^1.1.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=21.1.0"
  }
}
